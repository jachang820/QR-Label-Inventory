{{> nav }}

<div class="container">
  <h1>Order {{order.id}}</h1>
  <p>Invoice #: {{order.label}}</p>
  <p>Notes: {{order.notes}}</p>

  <table>
    <tr>
      <th>#</th>
      <th>ID</th>
      <th>Status</th>
      <th>SKU</th>
      <th>UPC</th>
      <th>Factory Order</th>
      <th>Customer Order</th>
      <th>Inner Carton</th>
      <th>Master Carton</th>
    </tr>

    {{#each order.Items}}
      <tr>
        <td>{{this.num}}</td>
        <td>{{this.id}}</td>
        <td>{{this.status}}</td>
        <td>{{this.SKU.id}}</td>
        <td>{{this.SKU.upc}}</td>
        <td>{{this.FactoryOrderId}}</td>
        <td>{{this.CustomerOrderId}}</td>
        <td>{{this.innerbox}}</td>
        <td>{{this.outerbox}}</td>
      </tr>
    {{/each}}

  </table>
</div>


